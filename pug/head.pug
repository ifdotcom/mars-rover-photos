mixin item(name, value)
	.data--card__items
		p=name
		span=value
-var title = "Mars Rover Photos"
-var subtitle = "Image data gathered by NASA's Curiosity, Opportunity, and Spirit rovers on Mars"
doctype html
html
	head
		meta(charset="UTF-8")
		meta(http-equiv="X-UA-Compatible", content="IE=edge")
		meta(name="viewport", content="width=device-width, initial-scale=1.0")
		link(rel="preconnect",href="https://fonts.googleapis.com")
		link(rel="preconnect",href="https://fonts.gstatic.com" crossorigin)
		link(rel="stylesheet",href="https://fonts.googleapis.com/css2?family=Orbitron:wght@700;800&family=Play&display=swap")
		link(rel="stylesheet", href="../css/styles.css")
		title=title
	body
		block contenido


